{
  "name": "next-routes-with-locale",
  "version": "1.3.4",
  "description": "Easy to use locale-based dynamic routes for Next.js",
  "repository": "vonschau/next-routes-with-locale",
  "main": "dist",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "standard 'src/*' 'test/*'",
    "build": "del dist/* && babel src -d dist",
    "prepublishOnly": "yarn test",
    "pretest": "npm run lint && npm run build",
    "test": "yarn testOnly",
    "testOnly": "jest \\.test.js --coverage --no-cache",
    "testCI": "yarn test && cat coverage/lcov.info | coveralls",
    "testWatch": "yarn test --watch",
    "dev": "concurrently -k 'npm run build -- -w' 'yarn testOnly -- --watch'"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "<rootDir>/.next/",
      "<rootDir>/node_modules/"
    ],
    "roots": [
      "test/",
      "dist/"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  },
  "dependencies": {
    "@babel/cli": "^7.0.0-beta.51",
    "@babel/core": "^7.0.0-beta.51",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.51",
    "babel-core": "7.0.0-bridge.0",
    "jest-cli": "^23.2.0",
    "path-to-regexp": "^2.2.0"
  },
  "peerDependencies": {
    "next": "^4.2.3",
    "react": "^16.2.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.0.0-beta.51",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^23.2.0",
    "babel-plugin-transform-define": "^1.3.0",
    "concurrently": "^3.5.1",
    "coveralls": "^3.0.0",
    "del-cli": "^1.1.0",
    "jest": "^23.2.0",
    "next": "^6.0.3",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-test-renderer": "^16.2.0",
    "standard": "^11.0.1"
  },
  "author": "fridays",
  "license": "MIT",
  "keywords": [
    "next",
    "next.js",
    "react",
    "route",
    "router",
    "routes",
    "routing"
  ]
}

{
  "name": "@join-com/next-routes-with-locale",
  "version": "2.1.1",
  "description": "Easy to use locale-based dynamic routes for Next.js",
  "repository": "https://github.com/join-com/next-routes-with-locale",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "sideEffects": false,
  "scripts": {
    "prepare": "yarn build",
    "prepublishOnly": "yarn test && yarn lint",
    "preversion": "npm run lint",
    "version": "yarn prettier && git add -A src",
    "postversion": "git push && git push --tags",
    "lint": "tslint -c tslint.json 'src/**/*.ts' 'src/**/*.tsx'",
    "lint:fix": "yarn lint --fix",
    "build": "del lib/* && tsc -p tsconfig.build.json",
    "test": "jest --forceExit --no-cache --detectOpenHandles",
    "prettier": "prettier --write \"src/**/*.{ts,tsx}\"",
    "ts:check": "tsc --noEmit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn ts:check && lint-staged && yarn test"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "tslint -c tslint.json --fix",
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "path-to-regexp": "^2.4.0",
    "qs": "^6.7.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "@types/node": "^12.6.8",
    "@types/qs": "^6.5.3",
    "@types/react": "^16.8.23",
    "@types/react-test-renderer": "^16.8.2",
    "del-cli": "^2.0.0",
    "husky": "^3.0.1",
    "jest": "^24.8.0",
    "lint-staged": "^9.2.0",
    "next": "^9.0.3",
    "prettier": "^1.18.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-test-renderer": "^16.8.6",
    "ts-jest": "^24.0.2",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.9.6"
  },
  "author": "JOIN",
  "license": "MIT"
}

{
  "name": "next-routes-with-locale",
  "version": "1.3.4",
  "description": "Easy to use locale-based dynamic routes for Next.js",
  "repository": "vonschau/next-routes-with-locale",
  "main": "dist",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "standard 'src/*' 'test/*'",
    "build": "del dist/* && babel src -d dist",
    "prepublishOnly": "yarn test",
    "pretest": "npm run lint && npm run build",
    "test": "yarn testOnly",
    "testOnly": "jest \\.test.js --coverage",
    "testCI": "yarn test && cat coverage/lcov.info | coveralls",
    "testWatch": "yarn test --watch",
    "dev": "concurrently -k 'npm run build -- -w' 'yarn testOnly -- --watch'"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "jest": {
    "roots": [
      "test/",
      "dist/"
    ]
  },
  "dependencies": {
    "path-to-regexp": "^2.2.0"
  },
  "peerDependencies": {
    "next": "^4.2.3",
    "react": "^16.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^23.0.0-alpha.0",
    "babel-preset-env": "^1.6.1",
    "concurrently": "^3.5.1",
    "coveralls": "^3.0.0",
    "del-cli": "^1.1.0",
    "jest": "^22.4.2",
    "next": "^4.2.3",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-test-renderer": "^16.2.0",
    "standard": "^11.0.1"
  },
  "author": "fridays",
  "license": "MIT",
  "keywords": [
    "next",
    "next.js",
    "react",
    "route",
    "router",
    "routes",
    "routing"
  ]
}
